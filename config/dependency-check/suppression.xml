<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for test dependencies -->
    <suppress>
        <notes><![CDATA[
            Suppress false positives in test scope dependencies
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/.*@.*test.*$</packageUrl>
    </suppress>
    
    <!-- Suppress known non-exploitable vulnerabilities -->
    <suppress>
        <notes><![CDATA[
            CVE-2021-44228 - Log4j - Not exploitable in our context
        ]]></notes>
        <cve>CVE-2021-44228</cve>
    </suppress>
    
    <!-- Suppress development dependencies -->
    <suppress>
        <notes><![CDATA[
            Development dependencies not in production
        ]]></notes>
        <packageUrl regex="true">^pkg:npm/.*@.*dev.*$</packageUrl>
    </suppress>
</suppressions>
